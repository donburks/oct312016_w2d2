<!doctype html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <h3><%= animal %></h3>
    <ul>
      <% for (var i = 0; i < 10; i++) { %>
        <% if (i == 7) { %>
            <li>SEVEN!!</li>
        <% } else { %>
          <li><%= i %></li>
        <% } %>
      <% } %>
    </ul>
  </body>
</html>
